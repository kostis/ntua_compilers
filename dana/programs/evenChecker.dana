def main

  def even: n as int, result as ref int
    if n % 2 == 0:
      result := 1
    else:
      result := 0

  var n is int
  var res is int

  n := readInteger()

  even: n, res

  writeInteger: n
  writeString: " is "
  if res == 0:
    writeString: "even!"
  else:
    writeString: "odd!"
  writeString: "\n"
