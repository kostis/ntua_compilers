fun main() : nothing

    fun fibo(x : int) : int
    {
        if (x = 0) then return 0;
        if (x = 1) then return 1;
        return fibo(x-1) + fibo(x-2);
    }

    var N,limit : int;

{
    limit <- 21;
    N <- 0;
    while (N <= limit)
    do {
        writeString("fibo(");
        writeInteger(N);
        writeString(") = ");
        writeInteger(fibo(N));
        writeString("\n");
        N <- N+1;
    }
}
